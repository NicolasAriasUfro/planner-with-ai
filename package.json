{
  "name": "planner-with-ai",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/app.ts --watch src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "smoke": "concurrently -k -s first -n \"API,CYP\" -c \"magenta,cyan\" \"npm run dev\" \"wait-on http://localhost:3000 && cypress run --spec \\\"cypress/e2e/smoke/*.cy.{js,ts}\\\"\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasAriasUfro/planner-with-ai.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NicolasAriasUfro/planner-with-ai/issues"
  },
  "homepage": "https://github.com/NicolasAriasUfro/planner-with-ai#readme",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^30.0.0",
    "concurrently": "^9.2.1",
    "cypress": "^15.5.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "wait-on": "^9.0.1"
  },
  "dependencies": {
    "@google/genai": "^1.26.0",
    "dotenv": "^17.2.3",
    "express": "^4.17.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/test/*.cy.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
